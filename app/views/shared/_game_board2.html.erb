<div id="game-main" style="position: relative;">
  <div id="game-settings" class="well" style="height: 41px; width: 150px; padding: 15px; padding-top: 4px">
    <h5 style="margin-top: 0px;">Current Turn</h5>
    <div id="turn-timer-box" style="float: left; width: 110px; position: relative; background-color: #DCDDDD; height: 28px; border-radius: 4px 4px 4px 4px;">
      <span id="turn-username" style="position: absolute; top: 5px; left: 5px; color: white;"></span>
      <div class="turn_progress progress progress-success progress-striped active" style="height: 28px;">
        <div class="bar" style="background-color: #009F9D; width: 100%; height: 28px;"></div>
      </div>
    </div>
    <%= link_to '<i style="margin-top: 2px;" class="icon-ok icon-white"></i>'.html_safe, end_turn_home_index_path(:game_name => @game.name), :remote => true, :id => "game-endturn", :class => "btn btn-success", :style => "display:none; float: right;" %>
  </div>

  <div id="game-map-info" class="well <%= map_vote_class(@my_votes, @map.id) %>" style="height: 41px; width: 174px; padding: 4px 3px 15px 3px;" data-map-id="<%= @map.id %>">
    <h5 style="margin-top: 0px;">
      Map:
      <% if (current_user != nil && current_user.admin?) %>
        <a href="<%= edit_map_url(@map.id) %>"><%= @map.name %></a>
      <% else %>
        <%=	@map.name %>
      <% end %>
    </h5>
    <table style="position: relative;">
      <tbody style="">
        <tr>
          <td style="width: 23px;">
            <span style="position: absolute; top: 7px; font-size: .6em;"><%= "#{vote_percentage(1)}%" %></span>
          </td>
          <td style="width: 125px;">
            <div id="map-voting" style="position: absolute; top: 1px; left: 26px; color: white; height: 27px; z-index: 999; width: 123px;">
              <table style="width: 100%; height: 100%;">
                <tbody>
                  <tr>
                    <td style="width: 33%; text-align: center;">
                      <a href="#" class="btn btn-mini btn-success vote" style="display: none;" data-vote="1"><i class="icon-white icon-thumbs-up"></i></a>
                    </td>
                    <td style="width: 33%; text-align: center; cursor: default;"><span id="vote-text">vote</span></td>
                    <td style="width: 33%; text-align: center;">
                      <a href="#" class="btn btn-mini btn-danger vote" style="display: none;" data-vote="0"><i class="icon-white icon-thumbs-down"></i></a>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div class="progress progress-striped" style="height: 28px; position: relative">
              <div class="bar" style="height: 28px; background-color: #f08080; position: absolute; width: 100%; top: 0px; left: 0px;"></div>
              <div class="bar" style="height: 28px; background-color: green; position: absolute; width: <%= "#{vote_percentage(1)}%" %>; top: 0px; left: 0px;"></div>
            </div>
          </td>
          <td>
            <span style="position: absolute; top: 7px; font-size: .6em;"><%= "#{vote_percentage(0)}%" %></span>
          </td>
        </tr>
      </tbody>
    </table>
  </div>

